{
    "name": "OpenVSwitch",
    "template_type": "docker",
    "compute_id": "local",
    "image": "abdulmelink/openvswitch:latest",
    "adapters": 16,
    "console_type": "telnet",
    "environment": {
        "SERVICE_TYPE": "openvswitch",
        "OVS_NUM_PORTS": "16",
        "OVS_USER_MODE": "yes",
        "OVS_DATAPATH_TYPE": "netdev",
        "OVS_BRIDGE_NAME": "br0",
        "SDN_CONTROLLER_HOST": "sdn-controller",
        "SDN_CONTROLLER_PORT": "6633",
        "OVS_PROTOCOL": "OpenFlow13",
        "OVS_FAIL_MODE": "secure",
        "NETWORK_MODE": "gns3",
        "USE_STATIC_IP": "true",
        "LOG_LEVEL": "DEBUG",
        "WAIT_FOR_SERVICES_TIMEOUT": "60",
        "WAIT_FOR_SERVICES_REQUIRED": "false",
        "SUBNET_PREFIX": "192.168.100",
        "CLIENT_IP_RANGE": "100-255",
        "SERVER_IP_RANGE": "10-19",
        "POLICY_IP_RANGE": "20-29",
        "CONTROLLER_IP_RANGE": "30-49",
        "OVS_IP_RANGE": "42-99",
        "COLLECTOR_IP": "40",
        "NODE_IP_FL_SERVER": "192.168.100.10",
        "NODE_IP_POLICY_ENGINE": "192.168.100.20",
        "NODE_IP_SDN_CONTROLLER": "192.168.100.41",
        "NODE_IP_COLLECTOR": "192.168.100.40",
        "NODE_IP_OPENVSWITCH": "192.168.100.42",
        "NODE_IP_FL_CLIENT_1": "192.168.100.101",
        "NODE_IP_FL_CLIENT_2": "192.168.100.102",
        "OVS_DPDK_ENABLED": "false",
        "OVS_ADD_ETH0": "true",
        "OVS_SET_BOOT_FLOWS": "true"
    },
    "start_command": "/app/entrypoint.sh",
    "category": "switch",
    "symbol": ":/symbols/classic/multilayer_switch.svg",
    "usage": "OpenVSwitch for SDN-based networks",
    "default_name_format": "openvswitch-{0}",
    "privileged": true
} 